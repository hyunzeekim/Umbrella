<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.umbrella.umbrella">
    <uses-permission android:name="android.permission.SEND_SMS" />

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CreatePasswordActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Welcome" />
        <activity android:name=".InputPasswordActivity" />
        <activity android:name=".ProgramActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".ChooseContacts" />

        <!-- <activity -->
        <!-- android:name=".ShapeSettings" -->
        <!-- android:label="@string/title_activity_shape_settings" /> -->

        <activity android:name=".TimerPage" />
        <activity android:name=".CheckIn" />
        <activity android:name=".Finish" />

    </application>

</manifest>